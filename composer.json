{
    "name" : "slimen/log-tracker",
    "description" : "Display symfony logs in more elegant way and track exceptions by sending real time mails containing details about the exception.",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "Arnaout slimen",
        "email" : "arnaout.slimen@sbc.tn"
    }],
    "keywords" : [
        "log track",
        "exceptions track",
        "log file"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": "^5.5.9",
        "symfony/framework-bundle": "^2.7 || ^3.0"
    },
    "autoload" : {
        "psr-0" : {
            "SBC\\LogTrackerBundle" : ""
        }
    },
    "target-dir" : "SBC/LogTrackerBundle",
    "repositories" : [{
    }],
    "extra" : {
    "branch-alias" : {
            "dev-master" : "1-dev"
        }
    }
}